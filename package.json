{
  "name": "better-edusoft",
  "version": "1.0.0",
  "main": "./dist/main/index.js",
  "license": "MIT",
  "scripts": {
    "dev-renderer": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "watch-main": "swc ./main -d ./dist/main -s -w",
    "watch-preload": "swc ./preload/preload.ts -o ./dist/preload.js -w",
    "watch": "concurrently \"npm:watch-*\" -p -c --kill-others-on-fail",
    "dev-main": "nodemon --exec \"electron ./dist/main/index.js\" --watch dist/main",
    "dev": "concurrently \"npm:watch\" \"npm:dev-*\" -p -c -l 3 --kill-others-on-fail",
    "start": "electron ./dist/main/index.js",
    "test-unit": "mocha"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.5",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.57",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/set-cookie-parser": "^2.4.2",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.2",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "^6.11.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "electron": "24.2.0",
    "node-html-parser": "^6.1.5",
    "react-redux": "^8.0.5",
    "set-cookie-parser": "^2.6.0"
  }
}
